Schema : 
-----------------------------------------------------------------------------------------------------

{
	_id			    : String,
	frequency       : String,
	company_ID      : String,
    createdAt	    : Date,
    createdBy       : String,
}

APIs :
-----------------------------------------------------------------------------------------------------
1. Insert new frequency									- 
2. Details of frequency									- 
3. List details of all the frequencys				    - 
4. Update frequency details                    		    - 
5. Delete particular frequency							- 
6. Delete all the frequency							    - 
7. List all frequency for specify company				- 


-----------------------------------------------------------------------------------------------------
								1. Insert new frequency
-----------------------------------------------------------------------------------------------------

Method      : POST
    
URL         : /api/frequency

Input       : 
				{
					frequency	    : String, // Mandatory Field
					company_ID      : String,
					createdBy		: String, // Either empty or _id of users
				}

Output      : 
				If successfully
					{
						message : "frequency Added",
                        ID      : String
					}

-----------------------------------------------------------------------------------------------------
								2. Details of frequency
-----------------------------------------------------------------------------------------------------

Method      : GET
    
URL         : /api/frequency/:frequency

Input       : - NA-

Output      : 
				If successfully
					{
						frequency   : String,
						createdAt	: Date,
                        createdBy   : String,
						_id         : String
					} 
					
-----------------------------------------------------------------------------------------------------
								3. List details of all the frequency
-----------------------------------------------------------------------------------------------------

Method      : GET
    
URL         : /api/frequency/list

Input       : - NA-

Output      : 
				If successfully
					[
						{
                            frequency       : String,
                            createdAt	    : Date,
                            createdBy       : String,
                            _id             : String
						}
					]	 
-----------------------------------------------------------------------------------------------------
								4. Update frequency details
-----------------------------------------------------------------------------------------------------

Method      : PUT
    
URL         : /api/frequency/

Input       : 
                {
                    id           : String,
                    frequency    : String,
                }

Output      : 
				If successfully 
					"Frequency Updated"	 
				If not 
					"Something went wrong."

-----------------------------------------------------------------------------------------------------
							5. Delete particular frequency
-----------------------------------------------------------------------------------------------------

Method      : DELETE
    
URL         : /api/frequency/:frequency
			
Input       : - NA -
				
Output      : 
				If successfully 
					"Frequency type deleted"

-----------------------------------------------------------------------------------------------------
							6. Delete all the frequency
-----------------------------------------------------------------------------------------------------

Method      : DELETE
    
URL         : /api/frequency

Input       : - NA -			

Output      : 
				If successfully 
					"All frequency Deleted"

-----------------------------------------------------------------------------------------------------
							7. List all frequency for specify company
-----------------------------------------------------------------------------------------------------

Method      : GET
    
URL         : /api/frequency/list/:company_ID

Input       : - NA-

Output      : 
				If successfully
					[
						{
                            frequency		: String,
                            createdAt	    : Date,
                            createdBy       : String,
                            _id             : String,
							corporate_ID    : String,
						}
					]